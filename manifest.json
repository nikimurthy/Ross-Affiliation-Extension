{
    "manifest_version": 3,
    "name": "Ross Affiliation Checker",
    "version": "1.0",
    "description": "Check the percentage of Ross-affiliated students from a list of uniqnames.",
    "permissions": [],
    "host_permissions": [
      "https://mcommunity.umich.edu/*"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "assets/icon48.png",
        "48": "assets/icon48.png",
        "128": "assets/icon128.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  